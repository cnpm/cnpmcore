{
  "extends": "@eggjs/tsconfig",
  "compilerOptions": {
    "outDir": "dist",
    "declaration": false,
    "resolveJsonModule": true,
    "useUnknownInCatchVariables": false,
    "baseUrl": "."
  },
  "exclude": [
    "node_modules",
    "test"
  ],
  "include": [
    "app/**/*.ts",
    "app/**/*.json",
    "config/*.ts",
    "typings",
    "app.ts"
  ]
}
