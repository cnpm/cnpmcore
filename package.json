{
  "name": "cnpmcore",
  "version": "0.0.2",
  "description": "npm core",
  "main": "index.js",
  "files": [
    "dist/**/*"
  ],
  "scripts": {
    "dev": "egg-bin dev",
    "debug": "egg-bin debug",
    "lint": "eslint . --ext .ts",
    "lint:fix": "eslint . --ext .ts --fix",
    "test": "npm run lint && npm run test-local",
    "test-local": "egg-bin test -r tsconfig-paths/register",
    "cov": "egg-bin cov -r tsconfig-paths/register",
    "ci": "npm run lint && npm run cov",
    "clean": "tsc -b --clean",
    "tsc": "npm run clean && tsc -p ./tsconfig.json",
    "tsc:prod": "rm -rf dist && tsc -p ./tsconfig.prod.json",
    "prepublishOnly": "npm run tsc:prod"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/cnpm/npmcore.git"
  },
  "egg": {
    "declarations": true,
    "typescript": true
  },
  "keywords": [
    "npm"
  ],
  "dependencies": {
    "@eggjs/tegg": "^0.1.1",
    "@eggjs/tegg-config": "^0.1.0",
    "@eggjs/tegg-controller-plugin": "^0.1.1",
    "@eggjs/tegg-eventbus-plugin": "^0.1.5",
    "@eggjs/tegg-orm-decorator": "^0.1.5",
    "@eggjs/tegg-orm-plugin": "^0.1.5",
    "@eggjs/tegg-plugin": "^0.1.1",
    "@eggjs/tsconfig": "^1.0.0",
    "egg": "^2.29.4",
    "leoric": "^1.9.0",
    "mysql": "^2.18.1",
    "mysql2": "^2.3.0",
    "bson-objectid": "^2.0.1"
  },
  "devDependencies": {
    "@types/mocha": "^9.0.0",
    "egg-bin": "^4.16.4",
    "egg-mock": "^3.26.0",
    "eslint": "^7.32.0",
    "eslint-config-egg": "^9.0.0",
    "typescript": "^4.3.5"
  },
  "author": "killagu",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/cnpm/npmcore/issues"
  },
  "homepage": "https://github.com/cnpm/npmcore#readme",
  "engines": {
    "node": ">= 14.0.0"
  }
}
